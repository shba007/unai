{
  "name": "@shba007/unai",
  "version": "0.1.1",
  "description": "Unified AI Adapter Library. For Ollama, Gemini, Openai",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shba007/unai-library.git"
  },
  "keywords": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "volta": {
    "node": "22.13.1"
  },
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0"
  },
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "prepare": "husky || true",
    "dev": "vitest dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "play": "dotenvx run -- jiti examples/input-image",
    "build": "unbuild",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && pnpm publish && git push --follow-tags",
    "clean": "rm -rf coverage dist",
    "unai": "jiti examples/input-image"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "confbox": "^0.1.8",
    "consola": "^3.4.0",
    "ofetch": "^1.4.1",
    "pathe": "^2.0.2",
    "std-env": "^3.8.0",
    "unstorage": "^1.14.4",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@dotenvx/dotenvx": "^1.34.0",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "changelogen": "^0.5.7",
    "eslint": "^9.19.0",
    "eslint-config-unjs": "^0.4.2",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "unbuild": "3.3.1",
    "vitest": "^3.0.5"
  }
}